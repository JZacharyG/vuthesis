@article{Chudnovsky2011,
	Abstract = {Let $H$ be a graph. If $G$ is an $n$-vertex simple graph that does not contain $H$ as a minor, what is the maximum number of edges that $G$ can have? This is at most linear in $n$, but the exact expression is known only for very few graphs $H$. For instance, when $H$ is a complete graph $K_t$ , the ``natural'' conjecture, $(t-2)n - \frac{1}{2}(t-1)(t-2)$, is true only for $t \le 7$ and wildly false for large $t$, and this has rather dampened research in the area. Here we study the maximum number of edges when $H$ is the complete bipartite graph ${K}_{2,t}$ . We show that in this case, the analogous ``natural'' conjecture, $\frac{1}{2} (t+1)(n-1)$, is (for all $t \ge 2$ ) the truth for infinitely many $n$. },
	Author = {Maria Chudnovsky and Bruce Reed and Paul Seymour},
	Doi = {http://dx.doi.org/10.1016/j.jctb.2010.09.001},
	Issn = {0095-8956},
	Journal = {J. Comb. Theory Ser. B},
	Keywords = {Extremal},
	Number = {1},
	Pages = {18 - 46},
	Title = {The edge-density for ${K}_{2,t}$ minors},
	Url = {http://www.sciencedirect.com/science/article/pii/S0095895610000900},
	Volume = {101},
	Year = {2011},
}

